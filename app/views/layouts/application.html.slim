doctype html
html
  head
    title
      | ShortcutKeyGame
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_pack_tag 'application', media: 'all'
    = javascript_pack_tag 'application'
    = analytics_init if Rails.env.production?
  body
    = render 'layouts/header'
    .container
      = yield
      = render 'layouts/footer'
